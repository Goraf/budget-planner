    "type": "project",
    "require": {
        "zendframework/zend-diactoros": "^1.8",
        "middlewares/fast-route": "^1.1",
        "relay/relay": "2.x@dev",
        "middlewares/request-handler": "^1.3",
        "php-di/php-di": "^6.0",
        "league/plates": "^3.3",
        "vlucas/phpdotenv": "^2.5",
        "illuminate/database": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "BudgetPlanner\\": "src/"
        }
